{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "\ueb99 {{ .UserName }} on",
          "foreground": "#45F1C2",
          "type": "session",
          "style": "plain"
        },
        {
          "options": {
            "folder_separator_icon": "/",
            "style": "full"
          },
          "template": " \uf07b {{ .Path }} ",
          "foreground": "#0CA0D8",
          "type": "path",
          "style": "plain"
        },
        {
          "options": {
            "fetch_upstream_icon": true,
            "branch_icon": ""
          },
          "template": "{{ .UpstreamIcon }} git:({{ .HEAD }}) {{ if gt .StashCount 0 }}, stash:({{ .StashCount }}){{ end }} ",
          "foreground": "#14A5AE",
          "powerline_symbol": "\ue0b0",
          "type": "git",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "\ue3bf ",
          "foreground": "#cd5e42",
          "type": "root",
          "style": "plain"
        },
        {
          "template": "# ",
          "foreground": "#CD4277",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 4
}
